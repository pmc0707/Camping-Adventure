<% layout("/layouts/boilerplate")%>
<body>
    <h3>Edit Your cotent</h3>
    <form method="POST" action="/listings/<%= listing._id %>?_method=PUT">
        <input name="listing[title]" value="<%=listing.title%>" type="text"/><br><br>
        <textarea name="listing[description]"><%=listing.description%></textarea><br><br>
        <input type="text" name="listing[image]" value="<%=listing.image%>" placeholder="image here" required>
        <input name="listing[price]" value="<%=listing.price%>" type="number" /><br><br>
        <input name="listing[country]" value="<%=listing.country%>"/><br><br>
        <input name="listing[location]" value="<%=listing.location%>" /><br><br>
        <button>EDIT</button>
    </form>
</body>
